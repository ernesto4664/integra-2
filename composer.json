{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ebess/advanced-nova-media-library": "^4.1",
        "emilianotisato/nova-tinymce": "^2.0",
        "epartment/nova-dependency-container": "^1.3",
        "firebase/php-jwt": "~4.0",
        "guzzlehttp/guzzle": "~7.0",
        "illuminate/auth": "*",
        "illuminate/bus": "*",
        "illuminate/console": "*",
        "illuminate/container": "*",
        "illuminate/contracts": "*",
        "illuminate/cookie": "*",
        "illuminate/database": "*",
        "illuminate/encryption": "*",
        "illuminate/http": "*",
        "illuminate/support": "*",
        "laravel/framework": "^10.10",
        "laravel/nova": "~4.1",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravie/serialize-queries": "^2.2",
        "lcobucci/jwt": "^3.2",
        "league/oauth2-server": "^7.4",
        "maatwebsite/excel": "^3.1",
        "maatwebsite/laravel-nova-excel": "^1.3",
        "meat/list-rut": "^1.0",
        "meat/nova-push-notification": "^1.0",
        "nyholm/psr7": "^1.8",
        "outl1ne/nova-color-field": "^2.0",
        "outl1ne/nova-multiselect-field": "^4.4",
        "phpseclib/phpseclib": "^3.0",
        "spatie/laravel-medialibrary": "^10.9",
        "symfony/psr-http-message-bridge": "^2.3",
        "titasgailius/search-relations": "^2.0",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Composer\\Semver\\": "src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Meat\\ListRut\\": "nova-components/ListRut/",
            "Meat\\NovaPushNotification\\": "nova-components/NovaPushNotification/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Meat\\ListRut\\": "nova-components/ListRut/",
            "Meat\\NovaPushNotification\\": "nova-components/NovaPushNotification/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/ListRut"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaPushNotification"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
